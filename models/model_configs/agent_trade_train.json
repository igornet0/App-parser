{   
    "epochs": 100,
    "batch_size": 150,
    "num_workers": 0,
    "lr": 1e-5,
    "weight_decay": 1e-4,
    "patience": 7,
    "mixed": true,
    "mixed_precision": true,
    "RANDOM_INDICATETS": true,

    "agents": [
        {
            "name": "agent_trade_5m_full_RP",

            "indecaters": {
                "SMA": {"period": "?", "column": "?"},
                "EMA": {"period": "?", "column": "?"},
                "RSI": {"period": "?"},
                "ATR": {"period": "?"},
                "MACD": {"fast": "?", "slow": "?", "signal": "?"},
                "STOCHASTIC_OSCILLATOR": {"period": "?", "smoothing": "?"},
                "BOLLINGER": {"period": "?", "num_std": "?"},
                "VWAP": {},
                "OBV": {},
                "MFI": {"period": "?"},
                "CRV": {"period": "?", "trading_days": 362880}
            },

            "type": "AgentTradeTime",
            "timetravel": "5m",
            "discription": "Agent for signal generation and trading execution in a sequence.",

            "model_parameters": {
                "datetime_format": "%m-%d %H:%M %w",
                "input_features": ["max", "min", "close", "volume"],
                "seq_len": 30,
                "d_model": 128,
                "n_heads": 8,
                "dropout": 0.3
            }
        }
    ],

    "schema": {
        "period": {
            "type": "integer",
            "minimum": 5,
            "maximum": 40
        },
        "column": {
            "type": "string",
            "enum": ["open", "max", "min", "close"]
        },
        "fast": {
            "type": "integer",
            "minimum": 5,
            "maximum": 100
        },
        "slow": {
            "type": "integer",
            "minimum": 5,
            "maximum": 100
        },
        "signal": {
            "type": "integer",
            "minimum": 5,
            "maximum": 100
        },
        "smoothing": {
            "type": "integer",
            "minimum": 1,
            "maximum": 100
        },
        "num_std": {
            "type": "integer",
            "minimum": 1,
            "maximum": 100
        }
    }
}